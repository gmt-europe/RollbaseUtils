<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="Application">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="RbObject">
                    <xs:sequence>
                        <xs:element name="DisplayName" type="xs:string"/>
                        <xs:element name="Props" type="Properties" minOccurs="0"/>
                        <xs:element name="Description" type="xs:string"/>

                        <xs:element name="DependentDefs" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element name="DataObjectDef" type="DependentDataObjectDefType" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="DataObjectDefs" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="DataObjectDef" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element ref="Menus" minOccurs="0"/>

                        <xs:element name="WebPageDefs" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="WebPageDef" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Portals">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="WebSite" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="LegacyReports" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element name="Report" type="LegacyReport" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Roles" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="Role" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="HostedFiles" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="HostedFile" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="BatchJobs" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="Event" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element ref="Permissions" minOccurs="0"/>

                        <xs:element name="RolePages" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element name="RolePage" type="RolePage" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Translations" type="Translations" minOccurs="0"/>

                        <xs:element ref="Tokens" minOccurs="0"/>

                        <xs:element ref="SeedRecords" minOccurs="0"/>

                        <xs:element name="CustomLogo" type="xs:base64Binary" minOccurs="0"/>
                    </xs:sequence>

                    <xs:attribute name="orderNo" type="xs:int" use="required"/>
                    <xs:attribute name="isSystem" type="xs:boolean" use="required"/>
                    <xs:attribute name="version" type="xs:int" use="required"/>
                    <xs:attribute name="packedId" type="xs:long" use="required"/>
                    <xs:attribute name="language" type="xs:string" use="required"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="SeedRecords">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="RbCollection">
                    <xs:sequence>
                        <xs:element name="DataObject" type="DataObject" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="Tokens">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="RbCollection">
                    <xs:sequence>
                        <xs:element name="Token" type="Token" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="Token">
        <xs:complexContent>
            <xs:extension base="RbElement">
                <xs:sequence>
                    <xs:element name="Lang" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>
                                At least one translation will exist. The first translation always is the one of the
                                base language.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="RbElement">
                                    <xs:attribute name="code" type="xs:string" use="required"/>
                                    <xs:attribute name="text" type="xs:string" use="required"/>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>

                <xs:attribute name="name" type="xs:string" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="RolePage">
        <xs:annotation>
            <xs:documentation>
                RolePage is not defined as a root element because it does not have a natural name. If this becomes
                an issue, try to find out what the proper natural name of this object is and apply the changes.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RbElement">
                <xs:attribute name="pageDefId" type="xs:long" use="required"/>
                <xs:attribute name="pageId" type="xs:long" use="required"/>
                <xs:attribute name="roleId" type="xs:long" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Permissions">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="RbCollection">
                    <xs:sequence>
                        <xs:element name="Permission" type="Permission" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="Permission">
        <xs:annotation>
            <xs:documentation>
                Permission is not defined as a root element because the whole set of permissions as a whole should
                be versioned; not the separate permissions.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RbElement">
                <xs:attribute name="resId" type="xs:long" use="required"/>
                <xs:attribute name="prId" type="xs:long" use="required"/>
                <xs:attribute name="actionsSet" type="xs:int" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="HostedFile">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="RbObject">
                    <xs:sequence>
                        <xs:element name="DisplayName" type="xs:string"/>
                        <xs:element name="Description" type="xs:string"/>
                        <xs:element name="Translations" type="Translations" minOccurs="0"/>
                        <xs:element name="RawData" type="xs:base64Binary"/>
                    </xs:sequence>

                    <xs:attribute name="fileName" type="xs:string" use="required"/>
                    <xs:attribute name="contentType" type="xs:string" use="required"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="Role">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="RbObject">
                    <xs:sequence>
                        <xs:element name="Name" type="xs:string"/>
                        <xs:element name="Description" type="xs:string"/>
                        <xs:element name="Translations" type="Translations" minOccurs="0"/>
                        <xs:element name="Props" type="Properties"/>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="LegacyReport">
        <xs:annotation>
            <xs:documentation>
                LegacyReport is not defined as a root element because it is not expected to appear.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RbObject">
                <xs:sequence>
                    <xs:element name="Name" type="xs:string"/>
                    <xs:element name="Description" type="xs:string"/>
                    <xs:element name="Props" type="Properties" minOccurs="0"/>
                    <xs:element name="RawData" type="xs:base64Binary" minOccurs="0"/>
                    <xs:element name="Columns" type="xs:string"/>
                    <xs:element name="Translations" type="Translations" minOccurs="0"/>
                </xs:sequence>

                <xs:attribute name="objDefId" type="xs:long" use="required"/>
                <xs:attribute name="reportType" type="xs:string" use="required"/>
                <xs:attribute name="isPrivate" type="xs:boolean" use="required"/>
                <xs:attribute name="fileName" type="xs:string" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="WebSite">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="RbObject">
                    <xs:sequence>
                        <xs:element name="SiteName" type="xs:string"/>
                        <xs:element name="Stylesheet" type="xs:string"/>
                        <xs:element name="Props" type="Properties"/>
                        <xs:element name="Description" type="xs:string"/>
                        <xs:element name="Header" type="xs:base64Binary" minOccurs="0"/>
                        <xs:element name="Footer" type="xs:base64Binary" minOccurs="0"/>
                        <xs:element name="WebPageDefs" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="WebPageDef" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="Translations" type="Translations" minOccurs="0"/>
                    </xs:sequence>

                    <xs:attribute name="isMain" type="xs:boolean" use="required"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="DependentDataFieldDef">
        <xs:annotation>
            <xs:documentation>
                DependentDataFieldDef is not defined as a root element because it's an integral part of the
                DependentDataObjectDefType object.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RbObject">
                <xs:sequence>
                    <xs:element name="DisplayLabel" type="xs:string"/>
                </xs:sequence>

                <xs:attribute name="fieldName" type="xs:string" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="DependantListView">
        <xs:annotation>
            <xs:documentation>
                DependantListView is not defined as a root element because it's an integral part of the
                DependentDataObjectDefType object.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RbObject">
                <xs:sequence>
                    <xs:element name="ViewName" type="xs:string"/>
                </xs:sequence>

                <xs:attribute name="orderNo" type="xs:int" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="RelationshipDef">
        <xs:annotation>
            <xs:documentation>
                RelationshipDef is not defined as a root element because it's an integral part of the DataObject object.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RbObject">
                <xs:sequence>
                    <xs:element name="Translations" type="Translations" minOccurs="0"/>
                </xs:sequence>

                <xs:attribute name="relName" type="xs:string" use="required"/>
                <xs:attribute name="objDef1" type="xs:string" use="optional"/>
                <xs:attribute name="objDef2" type="xs:string" use="required"/>
                <xs:attribute name="singularName1" type="xs:string" use="required"/>
                <xs:attribute name="pluralName1" type="xs:string" use="required"/>
                <xs:attribute name="singularName2" type="xs:string" use="required"/>
                <xs:attribute name="pluralName2" type="xs:string" use="required"/>
                <xs:attribute name="isMultiple1" type="xs:boolean" use="required"/>
                <xs:attribute name="isMultiple2" type="xs:boolean" use="required"/>
                <xs:attribute name="orphanControl1" type="xs:boolean" use="required"/>
                <xs:attribute name="orphanControl2" type="xs:boolean" use="required"/>
                <xs:attribute name="cloneRelated1" type="xs:boolean" use="required"/>
                <xs:attribute name="cloneRelated2" type="xs:boolean" use="required"/>
                <xs:attribute name="isSystem" type="xs:boolean" use="required"/>
                <xs:attribute name="isPkFk" type="xs:boolean" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="DependentDataObjectDef">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="DependentDataObjectDefType"/>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="DependentDataObjectDefType">
        <xs:annotation>
            <xs:documentation>
                DependentDataObjectDef is defined as a type here because there is a name conflict. In both
                DependentDefs and DataObjectDefs this object is named DataObjectDef. To be able to store
                DependentDefs\DataObjectDef separately, the root element DependentDataObjectDef is created which
                just inherits from this type.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RbObject">
                <xs:sequence>
                    <xs:element name="SingularName" type="xs:string"/>
                    <xs:element name="PluralName" type="xs:string"/>
                    <xs:element name="Description" type="xs:string"/>

                    <xs:element name="DataFieldDefs">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="RbCollection">
                                    <xs:sequence>
                                        <xs:element name="DataFieldDef" type="DependentDataFieldDef" minOccurs="0" maxOccurs="unbounded"/>
                                    </xs:sequence>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>

                    <xs:element name="ListViews" minOccurs="0">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="RbCollection">
                                    <xs:sequence>
                                        <xs:element name="ListView" type="DependantListView" minOccurs="0" maxOccurs="unbounded"/>
                                    </xs:sequence>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>

                    <xs:element name="RelationshipDefs" minOccurs="0">
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:extension base="RbCollection">
                                    <xs:sequence>
                                        <xs:element name="RelationshipDef" type="RelationshipDef" minOccurs="0" maxOccurs="unbounded"/>
                                    </xs:sequence>
                                </xs:extension>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>

                <xs:attribute name="objDefName" type="xs:string" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="DataObjectDef">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="RbObject">
                    <xs:sequence>
                        <xs:element name="Props" type="Properties" minOccurs="0"/>
                        <xs:element name="SingularName" type="xs:string" minOccurs="0"/>
                        <xs:element name="PluralName" type="xs:string" minOccurs="0"/>
                        <xs:element name="NameTemplate" type="xs:string" minOccurs="0"/>
                        <xs:element name="Description" type="xs:string" minOccurs="0"/>

                        <xs:element name="DataFieldDefs">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element name="DataFieldDef" type="DataFieldDef" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="RelationshipDefs">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element name="RelationshipDef" type="RelationshipDef" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="ListViews">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="ListView" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Actions" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element name="Action" type="Action" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Statuses" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element name="Status" type="Status" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Processes" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="Process" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Buttons" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="Button" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="ConversionMaps" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="ConversionMap" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Charts" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="Chart" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Gauges" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="Gauge" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Reports" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="Report" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Templates" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="Template" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Events" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="Event"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="Questions" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element ref="Question" minOccurs="0" maxOccurs="unbounded"/>
                                            <xs:element name="ListItem" type="ListItem" minOccurs="0" maxOccurs="unbounded"/>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>

                    <xs:attribute name="objDefName" type="xs:string" use="required"/>
                    <xs:attribute name="isSystem" type="xs:boolean" use="required"/>
                    <xs:attribute name="isAuditable" type="xs:boolean" use="required"/>
                    <xs:attribute name="isViewable" type="xs:boolean" use="required"/>
                    <xs:attribute name="isFlaggable" type="xs:boolean" use="required"/>
                    <xs:attribute name="isDependent" type="xs:boolean" use="required"/>
                    <xs:attribute name="isDeployed" type="xs:boolean" use="required"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="ConversionMap">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="RbObject">
                    <xs:sequence>
                        <xs:element name="MapNodes">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element name="MapNode" minOccurs="0" maxOccurs="unbounded">
                                                <xs:complexType>
                                                    <xs:complexContent>
                                                        <xs:extension base="RbElement">
                                                            <xs:attribute name="dest" type="xs:string" use="required"/>
                                                            <xs:attribute name="src" type="xs:string" use="required"/>
                                                            <xs:attribute name="const" type="xs:string" use="required"/>
                                                        </xs:extension>
                                                    </xs:complexContent>
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="CloneNodes" minOccurs="0">
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="RbCollection">
                                        <xs:sequence>
                                            <xs:element name="CloneNode" minOccurs="0" maxOccurs="unbounded">
                                                <xs:complexType>
                                                    <xs:complexContent>
                                                        <xs:extension base="RbElement">
                                                            <xs:attribute name="objDef" type="xs:string" use="required"/>
                                                            <xs:attribute name="srcRel" type="xs:string" use="required"/>
