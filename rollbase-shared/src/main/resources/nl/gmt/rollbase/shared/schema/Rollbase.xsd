<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="Application">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="DisplayName" type="xs:string"/>
                <xs:element name="Props" type="PropsEl" minOccurs="0"/>
                <xs:element name="Description" type="xs:string"/>

                <xs:element name="DependentDefs" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="DataObjectDef" type="DependentObjectDefEl" minOccurs="0"
                                        maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="DataObjectDefs" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="DataObjectDef" type="DataObjectDefEl" minOccurs="0"
                                        maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="Menus" type="MenusEl" minOccurs="0"/>
                <xs:element name="WebPageDefs" type="WebPageDefsEl" minOccurs="0"/>

                <xs:element name="Portals">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="WebSite" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="SiteName" type="xs:string"/>
                                        <xs:element name="Stylesheet" type="xs:string"/>
                                        <xs:element name="Props" type="PropsEl"/>
                                        <xs:element name="Description" type="xs:string"/>
                                        <xs:element name="Header" type="xs:base64Binary" minOccurs="0"/>
                                        <xs:element name="Footer" type="xs:base64Binary" minOccurs="0"/>
                                        <xs:element name="WebPageDefs" type="WebPageDefsEl" minOccurs="0"/>
                                        <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>
                                    </xs:sequence>

                                    <xs:attribute name="id" type="xs:string" use="required"/>
                                    <xs:attribute name="origId" type="xs:string" use="required"/>
                                    <xs:attribute name="isMain" type="xs:boolean" use="required"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="LegacyReports" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Report" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="Name" type="xs:string"/>
                                        <xs:element name="Description" type="xs:string"/>
                                        <xs:element name="Props" type="PropsEl" minOccurs="0"/>
                                        <xs:element name="RawData" type="xs:base64Binary" minOccurs="0"/>
                                        <xs:element name="Columns" type="xs:string"/>
                                        <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>
                                    </xs:sequence>

                                    <xs:attribute name="id" type="xs:string" use="required"/>
                                    <xs:attribute name="origId" type="xs:string" use="required"/>
                                    <xs:attribute name="objDefId" type="xs:long" use="required"/>
                                    <xs:attribute name="reportType" type="xs:string" use="required"/>
                                    <xs:attribute name="isPrivate" type="xs:boolean" use="required"/>
                                    <xs:attribute name="fileName" type="xs:string" use="required"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="Roles" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Role" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="Name" type="xs:string"/>
                                        <xs:element name="Description" type="xs:string"/>
                                        <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>
                                        <xs:element name="Props" type="PropsEl"/>
                                    </xs:sequence>

                                    <xs:attribute name="id" type="xs:string" use="required"/>
                                    <xs:attribute name="origId" type="xs:string" use="required"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="HostedFiles" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="HostedFile" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="DisplayName" type="xs:string"/>
                                        <xs:element name="Description" type="xs:string"/>
                                        <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>
                                        <xs:element name="RawData" type="xs:base64Binary"/>
                                    </xs:sequence>

                                    <xs:attribute name="id" type="xs:string" use="required"/>
                                    <xs:attribute name="origId" type="xs:string" use="required"/>
                                    <xs:attribute name="fileName" type="xs:string" use="required"/>
                                    <xs:attribute name="contentType" type="xs:string" use="required"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="BatchJobs" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Event" type="EventEl" minOccurs="0" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="Permissions" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Permission" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:attribute name="resId" type="xs:long" use="required"/>
                                    <xs:attribute name="prId" type="xs:long" use="required"/>
                                    <xs:attribute name="actionsSet" type="xs:int" use="required"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="RolePages" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="RolePage" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:attribute name="pageDefId" type="xs:long" use="required"/>
                                    <xs:attribute name="pageId" type="xs:long" use="required"/>
                                    <xs:attribute name="roleId" type="xs:long" use="required"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>

                <xs:element name="Tokens" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Token" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="Lang">
                                            <xs:complexType>
                                                <xs:attribute name="code" type="xs:string" use="required"/>
                                                <xs:attribute name="text" type="xs:string" use="required"/>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>

                                    <xs:attribute name="name" type="xs:string" use="required"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="SeedRecords" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="DataObject" type="DataObjectEl" minOccurs="0" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="CustomLogo" type="xs:base64Binary" minOccurs="0"/>
            </xs:sequence>

            <xs:attribute name="id" type="xs:string" use="required"/>
            <xs:attribute name="origId" type="xs:string" use="required"/>
            <xs:attribute name="orderNo" type="xs:int" use="required"/>
            <xs:attribute name="isSystem" type="xs:boolean" use="required"/>
            <xs:attribute name="version" type="xs:int" use="required"/>
            <xs:attribute name="packedId" type="xs:long" use="required"/>
            <xs:attribute name="language" type="xs:string" use="required"/>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="DependentObjectDefEl">

        <xs:sequence>
            <xs:element name="SingularName" type="xs:string"/>
            <xs:element name="PluralName" type="xs:string"/>
            <xs:element name="Description" type="xs:string"/>

            <xs:element name="DataFieldDefs">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="DataFieldDef" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="DisplayLabel" type="xs:string"/>
                                </xs:sequence>

                                <xs:attribute name="id" type="xs:string" use="required"/>
                                <xs:attribute name="origId" type="xs:string" use="required"/>
                                <xs:attribute name="fieldName" type="xs:string" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="ListViews" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="ListView" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="ViewName" type="xs:string"/>
                                </xs:sequence>

                                <xs:attribute name="id" type="xs:string" use="required"/>
                                <xs:attribute name="origId" type="xs:string" use="required"/>
                                <xs:attribute name="orderNo" type="xs:int" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="RelationshipDefs" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="RelationshipDef" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>
                                </xs:sequence>

                                <xs:attribute name="id" type="xs:string" use="required"/>
                                <xs:attribute name="origId" type="xs:string" use="required"/>
                                <xs:attribute name="relName" type="xs:string" use="required"/>
                                <xs:attribute name="objDef1" type="xs:string" use="optional"/>
                                <xs:attribute name="objDef2" type="xs:string" use="required"/>
                                <xs:attribute name="singularName1" type="xs:string" use="required"/>
                                <xs:attribute name="pluralName1" type="xs:string" use="required"/>
                                <xs:attribute name="singularName2" type="xs:string" use="required"/>
                                <xs:attribute name="pluralName2" type="xs:string" use="required"/>
                                <xs:attribute name="isMultiple1" type="xs:boolean" use="required"/>
                                <xs:attribute name="isMultiple2" type="xs:boolean" use="required"/>
                                <xs:attribute name="orphanControl1" type="xs:boolean" use="required"/>
                                <xs:attribute name="orphanControl2" type="xs:boolean" use="required"/>
                                <xs:attribute name="cloneRelated1" type="xs:boolean" use="required"/>
                                <xs:attribute name="cloneRelated2" type="xs:boolean" use="required"/>
                                <xs:attribute name="isSystem" type="xs:boolean" use="required"/>
                                <xs:attribute name="isPkFk" type="xs:boolean" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>

        <xs:attribute name="id" type="xs:string" use="required"/>
        <xs:attribute name="origId" type="xs:string" use="required"/>
        <xs:attribute name="objDefName" type="xs:string" use="required"/>
    </xs:complexType>

    <xs:complexType name="DataObjectDefEl">
        <xs:sequence>
            <xs:element name="Props" type="PropsEl" minOccurs="0"/>
            <xs:element name="SingularName" type="xs:string" minOccurs="0"/>
            <xs:element name="PluralName" type="xs:string" minOccurs="0"/>
            <xs:element name="NameTemplate" type="xs:string" minOccurs="0"/>
            <xs:element name="Description" type="xs:string" minOccurs="0"/>

            <xs:element name="DataFieldDefs">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="DataFieldDef" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <!-- These are shared -->

                                    <xs:element name="DisplayLabel" type="xs:string"/>
                                    <xs:element name="Description" type="xs:string" minOccurs="0"/>
                                    <xs:element name="Props" type="PropsEl" minOccurs="0"/>
                                    <xs:element name="ValidationScriptB64" type="xs:base64Binary" minOccurs="0"/>

                                    <!-- These are of select list -->

                                    <xs:element name="ListItems" minOccurs="0">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="ListItem" type="ListItemEl" minOccurs="0"
                                                            maxOccurs="unbounded"/>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>

                                    <!-- These are of shared image -->

                                    <xs:element name="ImageData" type="xs:base64Binary" minOccurs="0"/>

                                    <!-- These are shared -->

                                    <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>
                                </xs:sequence>

                                <xs:attribute name="id" type="xs:string" use="required"/>
                                <xs:attribute name="origId" type="xs:string" use="required"/>
                                <xs:attribute name="objDef" type="xs:string" use="required"/>
                                <xs:attribute name="columnName" type="xs:string" use="optional"/>
                                <xs:attribute name="fieldName" type="xs:string" use="required"/>
                                <xs:attribute name="groupName" type="xs:string" use="optional"/>
                                <xs:attribute name="dataClassName" type="xs:string" use="required"/>
                                <xs:attribute name="uiClassName" type="xs:string" use="required"/>
                                <xs:attribute name="isRequired" type="xs:boolean" use="required"/>
                                <xs:attribute name="isReadOnly" type="xs:boolean" use="required"/>
                                <xs:attribute name="isTextIndexable" type="xs:boolean" use="required"/>
                                <xs:attribute name="isSystem" type="xs:boolean" use="required"/>
                                <xs:attribute name="isAuditable" type="xs:boolean" use="required"/>
                                <xs:attribute name="hasPermissions" type="xs:boolean" use="required"/>
                                <xs:attribute name="maxLength" type="xs:int" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="RelationshipDefs">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="RelationshipDef" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>
                                </xs:sequence>

                                <xs:attribute name="id" type="xs:string" use="required"/>
                                <xs:attribute name="origId" type="xs:string" use="required"/>
                                <xs:attribute name="relName" type="xs:string" use="required"/>
                                <xs:attribute name="objDef1" type="xs:string" use="required"/>
                                <xs:attribute name="objDef2" type="xs:string" use="required"/>
                                <xs:attribute name="singularName1" type="xs:string" use="required"/>
                                <xs:attribute name="pluralName1" type="xs:string" use="required"/>
                                <xs:attribute name="singularName2" type="xs:string" use="required"/>
                                <xs:attribute name="pluralName2" type="xs:string" use="required"/>
                                <xs:attribute name="isMultiple1" type="xs:boolean" use="required"/>
                                <xs:attribute name="isMultiple2" type="xs:boolean" use="required"/>
                                <xs:attribute name="orphanControl1" type="xs:boolean" use="required"/>
                                <xs:attribute name="orphanControl2" type="xs:boolean" use="required"/>
                                <xs:attribute name="cloneRelated1" type="xs:boolean" use="required"/>
                                <xs:attribute name="cloneRelated2" type="xs:boolean" use="required"/>
                                <xs:attribute name="isSystem" type="xs:boolean" use="required"/>
                                <xs:attribute name="isPkFk" type="xs:boolean" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="ListViews">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="ListView" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="ViewName" type="xs:string"/>
                                    <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>
                                    <xs:element name="Props" type="PropsEl" minOccurs="0"/>
                                    <xs:element name="Columns" type="xs:string"/>
                                </xs:sequence>

                                <xs:attribute name="id" type="xs:string" use="required"/>
                                <xs:attribute name="origId" type="xs:string" use="required"/>
                                <xs:attribute name="orderNo" type="xs:int" use="required"/>
                                <xs:attribute name="objDefId" type="xs:string" use="required"/>
                                <xs:attribute name="isSystem" type="xs:boolean" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="Actions" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Action" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="DisplayName" type="xs:string"/>
                                    <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>
                                    <xs:element name="Props" type="PropsEl" minOccurs="0"/>
                                    <xs:element name="ConditionB64" type="xs:base64Binary" minOccurs="0"/>
                                </xs:sequence>

                                <xs:attribute name="id" type="xs:string" use="required"/>
                                <xs:attribute name="origId" type="xs:string" use="required"/>
                                <xs:attribute name="objDefId" type="xs:string" use="required"/>
                                <xs:attribute name="actionType" type="xs:string" use="required"/>
                                <xs:attribute name="orderNo" type="xs:int" use="required"/>
                                <xs:attribute name="pageId" type="xs:string" use="required"/>
                                <xs:attribute name="nextStatusId" type="xs:string" use="required"/>
                                <xs:attribute name="isGroup" type="xs:boolean" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="Statuses" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Status" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="DisplayName" type="xs:string"/>
                                    <xs:element name="SystemName" type="xs:string" minOccurs="0"/>
                                    <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>
                                </xs:sequence>

                                <xs:attribute name="id" type="xs:string" use="required"/>
                                <xs:attribute name="origId" type="xs:string" use="required"/>
                                <xs:attribute name="objDefId" type="xs:string" use="required"/>
                                <xs:attribute name="orderNo" type="xs:int" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="Processes" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Process" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="DisplayName" type="xs:string"/>
                                    <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>

                                    <xs:element name="Workflow">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="Node" minOccurs="0"
                                                            maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:attribute name="s" type="xs:string" use="required"/>
                                                        <xs:attribute name="a" type="xs:string" use="required"/>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>

                                <xs:attribute name="id" type="xs:string" use="required"/>
                                <xs:attribute name="origId" type="xs:string" use="required"/>
                                <xs:attribute name="objDefId" type="xs:string" use="required"/>
                                <xs:attribute name="orderNo" type="xs:int" use="required"/>
                                <xs:attribute name="defStatusId" type="xs:string" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="Buttons" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Buttons">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="DisplayName" type="xs:string"/>
                                    <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>
                                    <xs:element name="ScriptB64" type="xs:base64Binary"/>
                                    <xs:element name="Props" type="PropsEl" minOccurs="0"/>
                                </xs:sequence>

                                <xs:attribute name="id" type="xs:string" use="required"/>
                                <xs:attribute name="origId" type="xs:string" use="required"/>
                                <xs:attribute name="objDefId" type="xs:string" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="ConversionMaps" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="ConversionMap" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="MapNodes">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="MapNode" minOccurs="0" maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:attribute name="dest" type="xs:string" use="required"/>
                                                        <xs:attribute name="src" type="xs:string" use="required"/>
                                                        <xs:attribute name="const" type="xs:string" use="required"/>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>

                                    <xs:element name="CloneNodes" minOccurs="0">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="CloneNode" minOccurs="0" maxOccurs="unbounded">
                                                    <xs:complexType>
                                                        <xs:attribute name="objDef" type="xs:string" use="required"/>
                                                        <xs:attribute name="srcRel" type="xs:string" use="required"/>
                                                        <xs:attribute name="destRel" type="xs:string" use="required"/>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>

                                <xs:attribute name="id" type="xs:string" use="required"/>
                                <xs:attribute name="origId" type="xs:string" use="required"/>
                                <xs:attribute name="mapName" type="xs:string" use="required"/>
                                <xs:attribute name="destDefId" type="xs:string" use="required"/>
                                <xs:attribute name="srcDefId" type="xs:string" use="required"/>
                                <xs:attribute name="deleteSrc" type="xs:boolean" use="required"/>
                                <xs:attribute name="code" type="xs:string" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="Charts" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Chart" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="ChartName" type="xs:string"/>
                                    <xs:element name="Props" type="PropsEl" minOccurs="0"/>
                                    <xs:element name="Translations" type="TranslationsEl" minOccurs="0"/>
                                </xs:sequence>

                                <xs:attribute name="id" type="xs:string" use="required"/>
                                <xs:attribute name="origId" type="xs:string" use="required"/>
                                <xs:attribute name="orderNo" type="xs:int" use="required"/>
                                <xs:attribute name="objDefId" type="xs:long" use="required"/>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="Gauges" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
